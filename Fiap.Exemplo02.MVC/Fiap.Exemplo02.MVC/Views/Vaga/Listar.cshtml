@model List<Fiap.Exemplo02.MVC.Models.Vaga>
@{
    ViewBag.Title = "Listar";
}

<h2>Listar</h2>

<table class="table table-hover">
    <tr>
        <th>Cargo</th>
        <th>Descricao</th>
        <th>Salario</th>
        <th>VR</th>
        <th>Nivel</th>
    </tr>
        @foreach (var v in Model)
        {
            <tr>
                <td>@v.Cargo</td>
                <td>@v.Descricao</td>
                <td>@v.Salario</td>
                <td>@v.ValeRefeicao</td>
                <td>@v.Nivel</td>
            </tr>
          
        }
</table>
